{% macro includecss(href, chksum) %}
    <link rel="stylesheet", href="{{ href }}" {{ integrity="{{ chksum }}" if len(chksum) > 0 }} crossorigin=anonymous />
{% endmacro %}

{% macro includecss_multi(hrefs, chksums) %}
    {% for href in hrefs %}
        {{ includecss(href) }}
    {% endfor %}
{% endmacro %}


